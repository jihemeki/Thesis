    BoxLayout:
        size_hint_y: 10

        Label:
            size_hint_x: 30
            id: label
            text: 'Notification.'	

        Button:
            size_hint_x: 9
            text: 'Check'
            on_press: root.check()

        Button:
            size_hint_x: 9
            text: 'Hint'
            on_press: root.hint()

        Button:
            size_hint_x: 9
            text: 'Solve'
            on_press: root.solve()

        Button:
            size_hint_x: 9
            text: 'Clear'
            on_press: root.clear()

        Button:
            size_hint_x: 9
            text: 'Save'
            on_press: root.confirm()

        BoxLayout:
            size_hint_x: 15
            orientation: "vertical"

            Button:
                text: 'Solve Camera'
                on_press: root.cameraSolve()

            Button:
                text: 'Hint Camera'
                on_press: root.cameraHint()

        Button
            size_hint_x: 5
            text: "\/"
            on_press: root.show_load()


<Cell@tInput>:
    padding_x: self.width/2.5,0

<LoadDialog>:
    BoxLayout:
        size: root.size
        pos: root.pos
        orientation: "vertical"
        FileChooserListView:
            id: filechooser

        BoxLayout:
            size_hint_y: None
            height: 30
            Button:
                text: "Cancel"
                on_release: root.cancel()

            Button:
                text: "Load"
                on_release: root.load(filechooser.path, filechooser.selection)
